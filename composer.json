{
  "name": "barracudanetworks/jobrunner",
  "type": "library",
  "description": "A library to create and run jobs in PHP.",
  "keywords": ["cron", "daemons", "php"],
  "homepage": "https://github.com/barracudanetworks/jobrunner",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "psr/log": "1.0.0",
    "barracudanetworks/forkdaemon-php": "^1.0.10"
  },
  "autoload": {
    "psr-4": {
      "Barracuda\\JobRunner\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Barracuda\\JobRunner\\Examples\\": "examples"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.4",
    "codeclimate/php-test-reporter": "^0.2.0"
  }
}
